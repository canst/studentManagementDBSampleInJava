private JFrame frame;
    private void jbtnExitActionPerformed(java.awt.event.ActionEvent evt) {                                         
        frame = new JFrame("Exit");
        if (JOptionPane.showConfirmDialog(frame, "Confirm if you want to exit","School Management System",
            JOptionPane.YES_NO_OPTION )== JOptionPane.YES_NO_OPTION )
    {
        System.exit(0);
        }
    }                                        

    private void jbtnAddNewActionPerformed(java.awt.event.ActionEvent evt) {                                           
        try
        {
            Class.forName("com.mysql.jdbc.Driver");
            sqlConnector= DriverManager.getConnection( dataCon,username, password);
            pst = sqlCon.prepareStatement("insert into coursedb (StudentID, Firstname,Surname, Address,Gender,"
                + "DOB,Mobile,Email,Course, CourseCode, HomeStudent,OverseaStudent, Accommodation, Exchange, Scholarship, BA, BSc," +
                "MA,MSc,PhD, Module1, Module2, Module3, Module4, Module5, Module6, Module7, Module8," +
                "Score1, Score2, Score3, Score4, Score5, Score6, Score7, Score8, TotalScore, Ranking, Date)" 
                + "value(?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)");

            pst.setString(1,jtxtStudentID.getText());
            pst.setString(2,jtxtFirstname.getText());
            pst.setString(3,jtxtSurname.getText());
            pst.setString(4,jtxtAddress.getText());
            pst.setString(5,(String)jcboGender.getSelectedItem());
            pst.setString(6,jtxtDOB.getText());
            pst.setString(7,jtxtMobile.getText());
            pst.setString(8,jtxtEmail.getText());

            pst.setString(9,(String)jcboCourse.getSelectedItem());
            pst.setString(10,jtxtCourseCode.getText());
            pst.setString(11,(String)jcboHome.getSelectedItem());
            pst.setString(12,(String)jcboOversea.getSelectedItem());

            pst.setString(13,(String)jcboAccommodation.getSelectedItem());
            pst.setString(14,(String)jcboExchange.getSelectedItem());
            pst.setString(15,(String)jcboScholarship.getSelectedItem());
            
            pst.setString(16,(String)jcboBA.getSelectedItem());
            pst.setString(17,(String)jcboBSc.getSelectedItem());
            pst.setString(18,(String)jcboMA.getSelectedItem());
            pst.setString(19,(String)jcboMSc.getSelectedItem());
            pst.setString(20,(String)jcboPhD.getSelectedItem());
            
            pst.setString(21,(String)jcboModule1.getSelectedItem());
            pst.setString(22,(String)jcboModule2.getSelectedItem());
            pst.setString(23,(String)jcboModule3.getSelectedItem());
            pst.setString(24,(String)jcboModule4.getSelectedItem());
            pst.setString(25,(String)jcboModule5.getSelectedItem());
            pst.setString(26,(String)jcboModule6.getSelectedItem());
            pst.setString(27,(String)jcboModule7.getSelectedItem());
            pst.setString(28,(String)jcboModule8.getSelectedItem());
            
            pst.setString(29,(String)jtxtScore1.getText());
            pst.setString(30,(String)jtxtScore2.getText());
            pst.setString(31,(String)jtxtScore3.getText());
            pst.setString(32,(String)jtxtScore4.getText());
            pst.setString(33,(String)jtxtScore5.getText());
            pst.setString(34,(String)jtxtScore6.getText());
            pst.setString(35,(String)jtxtScore7.getText());
            pst.setString(36,(String)jtxtScore8.getText());
            
            pst.setString(37,(String)jtxtTotalScore.getText());
            pst.setString(38,(String)jtxtRanking.getText());
            pst.setString(39,(String)jtxtDate.getText());
            

            pst.executeUpdate();
            JOptionPane.showMessageDialog(this, "School Record Updated");
            uploadDB();

        }

        catch (ClassNotFoundException ex) {
            java.util.logging.Logger.getLogger(StdDB .class.getName()).log
            (java.util.logging.Level.SEVERE,null, ex);
        } catch (SQLException ex) {
            java.util.logging.Logger.getLogger(StdDB .class.getName()).log
            (java.util.logging.Level.SEVERE,null, ex);
        }

    }                                          

    private void jbtnResetActionPerformed(java.awt.event.ActionEvent evt) {                                          
               
        JTextField jtxt = null;
        JComboBox jcbo = null;

        // JTextField
        for (Component c:jPanel10.getComponents())
        {
            if (c.getClass().toString().contains("javax.swing.JTextField"))
            {
                jtxt = ( JTextField )c;
                jtxt.setText(null);
            }
        }

        for (Component c:jPanel5.getComponents())
        {
            if (c.getClass().toString().contains("javax.swing.JTextField"))
            {
                jtxt = ( JTextField )c;
                jtxt.setText(null);
            }
        }

        for (Component c:jPanel7.getComponents())
        {
            if (c.getClass().toString().contains("javax.swing.JTextField"))
            {
                jtxt = ( JTextField )c;
                jtxt.setText(null);
            }
        }
        
        for (Component c:jPanel4.getComponents())
        {
            if (c.getClass().toString().contains("javax.swing.JTextField"))
            {
                jtxt = ( JTextField )c;
                jtxt.setText(null);
            }
        }

        // JComboBox

        for (Component c:jPanel4.getComponents())
        {
            if (c.getClass().toString().contains("javax.swing.JComboBox"))
            {
                jcbo = ( JComboBox )c;
                jcbo.setSelectedIndex(0);
            }
        }

        for (Component c:jPanel8.getComponents())
        {
            if (c.getClass().toString().contains("javax.swing.JComboBox"))
            {
                jcbo = ( JComboBox )c;
                jcbo.setSelectedIndex(0);
            }
        }

        for (Component c:jPanel9.getComponents())
        {
            if (c.getClass().toString().contains("javax.swing.JComboBox"))
            {
                jcbo = ( JComboBox )c;
                jcbo.setSelectedIndex(0);
            }
        }

        jcboGender.setSelectedIndex(0);
        jcboCourse.setSelectedIndex(0);
        jcboGuidance.setSelectedIndex(0);
    }                                         

