    private void jcboCourseActionPerformed(java.awt.event.ActionEvent evt) {                                           
        if (jcboCourse.getSelectedItem().equals("BA Animatiom"))
        {
            jtxtCourseCode.setText("BA001");
            jtxtFaculty.setText("School of Art");
            jtxtDean.setText("Prof. Daniel Moore");
            jtxtLeader.setText("Dr. Pat Carlton");
            jtxtTutor.setText("Dr. Tony Moore");
        }

        else if (jcboCourse.getSelectedItem().equals("BSc Serious Games"))
        {
            jtxtCourseCode.setText("BSc001");
            jtxtFaculty.setText("School of Computer Games");
            jtxtDean.setText("Prof. Alfredo France");
            jtxtLeader.setText("Dr. Kate Peters");
            jtxtTutor.setText("Dr. Mike Normal");
        }
        
        else if (jcboCourse.getSelectedItem().equals("BSc Computer Science"))
            {
                jtxtCourseCode.setText("BScC709");
                jtxtFaculty.setText("School of Computer Science");
                jtxtDean.setText ( "Prof. Danny Moorehouse");
                jtxtLeader.setText ( "Prof. Felix Frognal");
                jtxtTutor.setText ( "Dr. Kelvin Kent");
            }
        
         else if (jcboCourse.getSelectedItem().equals("BSc Computing"))
            {
                jtxtCourseCode.setText("BScC025");
                jtxtFaculty.setText("School of Computer Science");
                jtxtDean.setText("Prof. Danny Moorehouse");
                jtxtLeader.setText("Prof. Kim Marie");
                jtxtTutor.setText("Dr. Johnny Davis");
            }
         
          else if (jcboCourse.getSelectedItem().equals("BSc Computer Animation"))
            {
                jtxtCourseCode.setText("BScCA976");
                jtxtFaculty.setText("School of Computer Animation");
                jtxtDean.setText("Prof. Danny Moorehouse");
                jtxtLeader.setText("Prof. Felix Stonehead");
                jtxtTutor.setText("Dr. Harry Lighthouse");
            }
           else if (jcboCourse.getSelectedItem().equals( "BSc Information System"))
            {
                jtxtCourseCode.setText( "BScC709");
                jtxtFaculty.setText("School of Computer Animation");
                jtxtDean.setText("Prof. Danny Moorehouse");
                jtxtLeader.setText("Prof. Felix Stonehead");
                jtxtTutor.setText("Dr. Harry Lighthouse");
            }

        else if (jcboCourse.getSelectedItem().equals("Select a Course"))
        {
            JTextField jtxt = null;
            for (Component c:jPanel7.getComponents())
        {
            if (c.getClass().toString().contains("javax.swing.JTextField"))
            {
                jtxt = ( JTextField )c;
                jtxt.setText(null);
            }
        }
        }
    }                                          

