    private void jcboModule2ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        if (jcboModule2.getSelectedItem().equals(""))
        {
            jtxtScore2.setText("0");
            jtxtScore2.setEnabled(false);
        }
        else
        {
           jtxtScore2.setText("");
           jtxtScore2.setEnabled(true); 
           jtxtScore2.requestFocus();
        }
    }                                           

